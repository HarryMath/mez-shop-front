{
  "hosting": {
    "site": "mez-shop-beta",
    "public": "dist/mez-shop",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
        "source": "/public/**",
        "destination": "/public.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }]
  }
}
